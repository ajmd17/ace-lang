module loop {
	docall: Function = (fun: Function) {
		lol: Function = (arg1) {
			print "in lol() arg1 == ", arg1
		}
	
		fun(lol)
	}
	
	type MyGeneric <T, J> {
		name: T
		sayhi = () {
			print "hi!!!"
		}
	}
	
	
	type Range<T> {
		start : T
		end   : T
		lol   : Int
		
		fun = () {
			t: T
			print "t == ", t
			print "Hi"
		}
	}
	
	range: Range<Int>
	range.start = 50
	range.end   = 1000
	
	range.fun()
	
	forloop : Function = (the_range: Range<Any>) {
		let start = the_range.start
		let end   = the_range.end
		
		print 'end - start == ', end - start
	}

	forloop(range)
	
