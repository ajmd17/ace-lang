module Example_Varargs {
  test: String = 'hello'

  sum: Function = (args...) {
    sum: Any = 0

    print test

    i: Int = 0
    len: Int = args.length()
    
    while i < len {
      sum += args[i]
      i += 1
    }

    return sum
  }

  print "sum result: ", sum(2, 4, 6, 8, 10, 12, 14, 16, 18)
}